{
  "manifest_version": 3,
  "name": "Color Ficker",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "icons": {
    "16": "images/icons.png",
    "48": "images/icons.png",
    "128": "images/icons.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "tabs", "storage", "scripting"],
  "action": {
    "default_popup": "popup.html"
  }
}
